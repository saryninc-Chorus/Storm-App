{
  "name": "acolyteschorus",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "ajv": "^8.17.1",
  "react": "^18.3.1",
  "react-dom": "^18.3.1",
    "react-router": "^6.25.1",
    "react-router-dom": "^6.25.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:poll": "CHOKIDAR_USEPOLLING=true CHOKIDAR_INTERVAL=200 react-scripts start",
    "clean": "rimraf node_modules package-lock.json && npm install",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
  "predeploy": "npm run build",
  "deploy": "gh-pages -d build",
  "deploy:netlify": "npm run build && echo 'Ready to deploy: upload build/ to Netlify.'",
  "deploy:vercel": "npm run build && echo 'Ready to deploy: upload build/ to Vercel.'",
  "safe-deploy": "pkill -f react-scripts || true && npm run build && npm run deploy"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^6.3.0"
  }
}
